<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>net.nausicaea.backup</string>
        <key>ProgramArguments</key>
        <array>
            <string>/opt/homebrew/bin/restic</string>
            <string>-v</string>
            <string>--no-lock</string>
            <string>backup</string>
            <string>--exclude-caches</string>
            <string>/Users/kitsune/Crypt</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Minute</key>
            <integer>11</integer>
        </dict>
        <key>ProcessType</key>
        <string>Background</string>
        <key>StandardOutPath</key>
        <string>/Users/kitsune/Library/Logs/net.nausicaea.backup.log</string>
        <key>StandardErrorPath</key>
        <string>/Users/kitsune/Library/Logs/net.nausicaea.backup.log</string>
    </dict>
</plist>

