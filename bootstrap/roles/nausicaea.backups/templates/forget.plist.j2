<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
 <dict>
  <key>Label</key>
  <string>net.nausicaea.forget</string>
  <key>ProgramArguments</key>
  <array>
      <string>/opt/homebrew/bin/restic</string>
      <string>-v</string>
      <string>forget</string>
      <string>--prune</string>
      <string>-H=6</string>
      <string>-d=4</string>
      <string>-w=2</string>
      <string>-m=6</string>
      <string>-y=10</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StartCalendarInterval</key>
  <dict>
      <key>Hour</key>
      <integer>9</integer>
      <key>Minute</key>
      <integer>45</integer>
  </dict>
  <key>ProcessType</key>
  <string>Background</string>
  <key>StandardOutPath</key>
  <string>/Users/kitsune/Library/Logs/net.nausicaea.forget.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/kitsune/Library/Logs/net.nausicaea.forget.log</string>
 </dict>
</plist>

